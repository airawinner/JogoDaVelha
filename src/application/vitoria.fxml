<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ControladorVelha">
    <children>
        <VBox fx:id="background" alignment="CENTER" onMouseClicked="#AcaoDoTabuleiro" prefHeight="500.0" prefWidth="500.0">
            <children>
                <ImageView fx:id="vitoria" fitHeight="600.0" fitWidth="600.0" onMouseClicked="#AcaoDoTabuleiro" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../img/vitória.png" />
                    </image>
                </ImageView>
            </children>
        </VBox>
        <VBox fx:id="Xframes" alignment="CENTER" mouseTransparent="True" onMouseClicked="#AcaoDoTabuleiro" prefHeight="500.0" prefWidth="500.0" />
          <ImageView fx:id="sair" fitHeight="71.0" fitWidth="108.0" onMouseClicked="#AcaoDoTabuleiro" pickOnBounds="true" preserveRatio="true" translateX="50.0" translateY="160.0">
              <image>
                  <Image url="@../../img/icone_sair-re.png" />
              </image>
          </ImageView>
      <ImageView fx:id="reiniciar" fitHeight="73.0" fitWidth="122.0" onMouseClicked="#AcaoDoTabuleiro" pickOnBounds="true" preserveRatio="true" translateY="170.0">
         <image>
            <Image url="@../../img/icone_reiniciar-re.png" />
         </image>
      </ImageView>
      <ImageView fx:id="velha" fitHeight="600.0" fitWidth="600.0" onMouseClicked="#AcaoDoTabuleiro" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img/empate.png" />
         </image>
      </ImageView>
      <Label fx:id="vencedor" text="O vencedor  é " textFill="#2b56a6" translateY="-170.0">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
    </children>
</StackPane>
